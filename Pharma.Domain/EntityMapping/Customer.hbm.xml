<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Customer" table="Customers">
    <id name="CustomerId" type="Int32">
      <column name="CustomerId" not-null="true" precision="10" scale="0" />
      <generator class="identity" />
    </id>
    <property name="CustomerName" type="String">
      <column name="CustomerName" default="NULL" not-null="false" length="25" />
    </property>
    <property name="CreationDate" type="DateTime">
      <column name="CreationDate" default="NULL" not-null="false" />
    </property>
    <property name="ModifyDate" type="DateTime">
      <column name="ModifyDate" default="NULL" not-null="false" />
    </property>
    <property name="CreatedByUser" type="String">
      <column name="CreatedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <property name="ModifiedByUser" type="String">
      <column name="ModifiedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <many-to-one name="Customertype" class="CustomerType">
      <column name="CustomerTypeId" not-null="true" precision="10" scale="0" />
    </many-to-one>
    <set name="Addresses" inverse="true" generic="true">
      <key>
        <column name="CustomerId" />
      </key>
      <one-to-many class="Address" />
    </set>
    <set name="Customercontacts" inverse="true" generic="true">
      <key>
        <column name="CustomerId" />
      </key>
      <one-to-many class="CustomerContact" />
    </set>
    <set name="Invoices" inverse="true" generic="true">
      <key>
        <column name="CustomerId" />
      </key>
      <one-to-many class="Invoice" />
    </set>
    <set name="Phonenumbers" inverse="true" generic="true">
      <key>
        <column name="CustomerId" />
      </key>
      <one-to-many class="PhoneNumber" />
    </set>
  </class>
</hibernate-mapping>
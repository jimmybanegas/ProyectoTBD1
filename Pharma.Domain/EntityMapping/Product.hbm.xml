<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Product" table="Products">
    <id name="ProductId" type="Int32">
      <column name="ProductId" not-null="true" precision="10" scale="0" />
      <generator class="guid" />
    </id>
    <property name="ProductName" type="String">
      <column name="ProductName" default="NULL" not-null="false" length="25" />
    </property>
    <property name="RetailPrice" type="Single">
      <column name="RetailPrice" default="NULL" not-null="false" precision="24" />
    </property>
    <property name="Cost" type="Single">
      <column name="Cost" default="NULL" not-null="false" precision="24" />
    </property>
    <property name="MinimumExistence" type="Int64">
      <column name="MinimumExistence" default="NULL" not-null="false" precision="19" scale="0" />
    </property>
    <property name="WholeSalerPercentage" type="Single">
      <column name="WholeSalerPercentage" default="NULL" not-null="false" precision="24" />
    </property>
    <property name="ExpireDate" type="DateTime">
      <column name="ExpireDate" default="NULL" not-null="false" />
    </property>
    <property name="CreationDate" type="DateTime">
      <column name="CreationDate" default="NULL" not-null="false" />
    </property>
    <property name="ModifyDate" type="DateTime">
      <column name="ModifyDate" default="NULL" not-null="false" />
    </property>
    <property name="CurrentExistence" type="Int32">
      <column name="CurrentExistence" default="NULL" not-null="false" precision="10" scale="0" />
    </property>
    <property name="CreatedByUser" type="String">
      <column name="CreatedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <property name="ModifiedByUser" type="String">
      <column name="ModifiedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <many-to-one name="Category" class="Category">
      <column name="CategoryId" not-null="true" precision="10" scale="0" />
    </many-to-one>
    <many-to-one name="Presentation" class="Presentation">
      <column name="PresentationId" not-null="true" precision="10" scale="0" />
    </many-to-one>
    <set name="Estimatedetails" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="EstimateDetail" />
    </set>
    <set name="Inventoryadjustmentdetails" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="InventoryaAjustmentDetail" />
    </set>
    <set name="Invoicedetails" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="InvoiceDetail" />
    </set>
    <set name="Kardexoperations" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="KardexOperation" />
    </set>
    <set name="Providersellsproducts" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="ProviderSellsProduct" />
    </set>
    <set name="Purchasedetails" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="PurchaseDetail" />
    </set>
    <set name="Purchaseorderdetails" inverse="true" generic="true">
      <key>
        <column name="ProductId" />
      </key>
      <one-to-many class="PurchaseOrderDetail" />
    </set>
  </class>
</hibernate-mapping>
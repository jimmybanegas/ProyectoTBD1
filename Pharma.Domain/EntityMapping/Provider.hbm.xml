<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Provider" table="Providers">
    <id name="ProviderId" type="Int32">
      <column name="ProviderId" not-null="true" precision="10" scale="0" />
      <generator class="identity" />
    </id>
    <property name="Name" type="String">
      <column name="Name" default="NULL" not-null="false" length="25" />
    </property>
    <property name="Phone" type="String">
      <column name="Phone" default="NULL" not-null="false" length="25" />
    </property>
    <property name="ContactName" type="String">
      <column name="ContactName" default="NULL" not-null="false" length="25" />
    </property>
    <property name="Address" type="String">
      <column name="Address" default="NULL" not-null="false" length="25" />
    </property>
    <property name="CreatedByUser" type="String">
      <column name="CreatedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <property name="ModifiedByUser" type="String">
      <column name="ModifiedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <property name="ModifyDate" type="DateTime">
      <column name="ModifyDate" default="NULL" not-null="false" />
    </property>
    <property name="CreationDate" type="DateTime">
      <column name="CreationDate" default="NULL" not-null="false" />
    </property>
    <set name="Priceestimates" inverse="true" generic="true">
      <key>
        <column name="ProviderId" />
      </key>
      <one-to-many class="PriceEstimate" />
    </set>
    <set name="Providersellsproducts" inverse="true" generic="true">
      <key>
        <column name="ProviderId" />
      </key>
      <one-to-many class="ProviderSellsProduct" />
    </set>
    <set name="Purchases" inverse="true" generic="true">
      <key>
        <column name="ProviderId" />
      </key>
      <one-to-many class="Purchase" />
    </set>
    <set name="Purchaseorders" inverse="true" generic="true">
      <key>
        <column name="ProviderId" />
      </key>
      <one-to-many class="PurchaseOrder" />
    </set>
  </class>
</hibernate-mapping>
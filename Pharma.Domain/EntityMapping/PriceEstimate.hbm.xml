<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="PriceEstimate" table="PriceEstimates">
    <id name="PriceEstimateId" type="Int32">
      <column name="PriceEstimateId" not-null="true" precision="10" scale="0" />
      <generator class="identity" />
    </id>
    <property name="EstimateDate" type="Int32">
      <column name="EstimateDate" default="NULL" not-null="false" precision="10" scale="0" />
    </property>
    <property name="EstimateTotal" type="Single">
      <column name="EstimateTotal" default="NULL" not-null="false" precision="24" />
    </property>
    <property name="CreationDate" type="DateTime">
      <column name="CreationDate" default="NULL" not-null="false" />
    </property>
    <property name="ModifyDate" type="DateTime">
      <column name="ModifyDate" default="NULL" not-null="false" />
    </property>
    <property name="CreatedByUser" type="String">
      <column name="CreatedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <property name="ModifiedByUser" type="String">
      <column name="ModifiedByUser" default="NULL" not-null="false" length="25" />
    </property>
    <many-to-one name="Provider" class="Provider">
      <column name="ProviderId" not-null="true" precision="10" scale="0" />
    </many-to-one>
    <set name="Estimatedetails" inverse="true" generic="true">
      <key>
        <column name="PriceEstimateId" />
      </key>
      <one-to-many class="EstimateDetail" />
    </set>
  </class>
</hibernate-mapping>
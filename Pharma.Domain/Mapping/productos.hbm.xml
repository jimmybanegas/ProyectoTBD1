<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="productos" table="productos" schema="farmacia">
    <id name="cod_prod" type="String">
      <column name="cod_prod" not-null="true" length="20" sql-type="varchar" />
      <generator class="assigned" />
    </id>
    <property name="existencia" type="Int32">
      <column name="existencia" not-null="false" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="fecha_venc" type="DateTime">
      <column name="fecha_venc" not-null="false" sql-type="date" />
    </property>
    <property name="costo" type="Double">
      <column name="costo" not-null="false" sql-type="double" />
    </property>
    <property name="precio_consumi" type="Double">
      <column name="precio_consumi" not-null="false" sql-type="double" />
    </property>
    <property name="precio_mayor" type="Double">
      <column name="precio_mayor" not-null="false" sql-type="double" />
    </property>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="45" sql-type="varchar" />
    </property>
    <!--<property name="cod_presentacion" type="Int32">
      <column name="cod_presentacion" not-null="false" precision="11" scale="0" sql-type="int" />
    </property>-->
    <property name="existencia_min" type="Int32">
      <column name="existencia_min" not-null="false" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="existencia_max" type="Int32">
      <column name="existencia_max" not-null="false" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="porcentaje_m" type="Int32">
      <column name="porcentaje_m" not-null="false" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <many-to-one name="presentacion_productos" class="presentacion_productos">
      <column name="cod_presentacion" not-null="false" precision="11" scale="0" sql-type="int" />
    </many-to-one>
    <set name="costo_prod_proveedors" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="costo_prod_proveedor" />
    </set>
    <set name="detalle_ajustes" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="detalle_ajustes" />
    </set>
    <set name="detalle_compras" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="detalle_compras" />
    </set>
    <set name="detalle_cotizaciones" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="detalle_cotizaciones" />
    </set>
    <set name="detalle_facturas" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="detalle_facturas" />
    </set>
    <set name="detalle_ocs" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="detalle_oc" />
    </set>
    <set name="kardexes" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="kardex" />
    </set>
    <set name="productos_has_categoria_productos" inverse="true" generic="true">
      <key>
        <column name="cod_prod" />
      </key>
      <one-to-many class="productos_has_categoria_productos" />
    </set>
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="tipo_transacciones" table="tipo_transacciones" schema="farmacia">
    <id name="id_tipotran" type="Int32">
      <column name="id_tipotran" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="15" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="15" sql-type="varchar" />
    </property>
    <property name="entrada_o_salida" type="String">
      <column name="entrada_o_salida" not-null="false" length="1" sql-type="varchar" />
    </property>
    <set name="ajustes" inverse="true" generic="true">
      <key>
        <column name="id_tipotran" />
      </key>
      <one-to-many class="ajustes" />
    </set>
  </class>
</hibernate-mapping>
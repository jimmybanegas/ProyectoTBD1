<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ajustes" table="ajustes" schema="farmacia">
    <id name="id_ajuste" type="Int32">
      <column name="id_ajuste" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="fecha" type="DateTime">
      <column name="fecha" not-null="false" sql-type="date" />
    </property>
    <property name="cant_producto" type="Int32">
      <column name="cant_producto" not-null="false" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="id_tipotran" type="Int32">
      <column name="id_tipotran" not-null="true" precision="11" scale="0" sql-type="int" />
    </property>
    <many-to-one name="tipo_transacciones" class="tipo_transacciones">
      <column name="id_tipotran" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
    <set name="detalle_ajustes" inverse="true" generic="true">
      <key>
        <column name="id_ajuste" />
      </key>
      <one-to-many class="detalle_ajustes" />
    </set>
  </class>
</hibernate-mapping>
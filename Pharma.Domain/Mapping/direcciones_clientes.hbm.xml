<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="direcciones_clientes" table="direcciones_clientes" schema="farmacia">
    <composite-id>
      <key-property name="direccion" type="String">
        <column name="direccion" not-null="true" length="50" sql-type="varchar" />
      </key-property>
      <key-property name="id_cliente" type="Int32">
        <column name="id_cliente" default="0" not-null="true" precision="11" scale="0" sql-type="int" />
      </key-property>
    </composite-id>
    <many-to-one name="clientes" class="clientes">
      <column name="id_cliente" default="0" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="contactos_clientes" table="contactos_clientes" schema="farmacia">
    <composite-id>
      <key-property name="nombre" type="String">
        <column name="nombre" not-null="true" length="50" sql-type="varchar" />
      </key-property>
      <key-property name="correo" type="String">
        <column name="correo" not-null="true" length="20" sql-type="varchar" />
      </key-property>
      <key-property name="telefono" type="String">
        <column name="telefono" not-null="true" length="15" sql-type="varchar" />
      </key-property>
      <key-property name="id_cliente" type="Int32">
        <column name="id_cliente" not-null="true" precision="11" scale="0" sql-type="int" />
      </key-property>
    </composite-id>
    <many-to-one name="clientes" class="clientes">
      <column name="id_cliente" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="contactos_clientes" table="contactos_clientes" schema="farmacia">
    <composite-id>
      <key-property name="id_contactocliente" type="Int32">
        <column name="id_contactocliente" not-null="true" precision="11" scale="0" sql-type="int" />
      </key-property>
      <key-property name="id_cliente" type="Int32">
        <column name="id_cliente" not-null="true" precision="11" scale="0" sql-type="int" />
      </key-property>
    </composite-id>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="50" sql-type="varchar" />
    </property>
    <property name="correo" type="String">
      <column name="correo" not-null="false" length="20" sql-type="varchar" />
    </property>
    <property name="telefono" type="String">
      <column name="telefono" not-null="false" length="15" sql-type="varchar" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <many-to-one name="clientes" class="clientes">
      <column name="id_cliente" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
  </class>
</hibernate-mapping>
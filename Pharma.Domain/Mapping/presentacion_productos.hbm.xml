<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="presentacion_productos" table="presentacion_productos" schema="farmacia">
    <id name="cod_presentacion" type="Int32">
      <column name="cod_presentacion" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="45" sql-type="varchar" />
    </property>
    <set name="productos" inverse="true" generic="true">
      <key>
        <column name="cod_presentacion" />
      </key>
      <one-to-many class="productos" />
    </set>
  </class>
</hibernate-mapping>
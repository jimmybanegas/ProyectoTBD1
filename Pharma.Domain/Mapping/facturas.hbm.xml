<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="facturas" table="facturas" schema="farmacia">
    <id name="num_factura" type="Int32">
      <column name="num_factura" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="assigned" />
    </id>
    <property name="fecha" type="DateTime">
      <column name="fecha" not-null="false" sql-type="date" />
    </property>
    <property name="subtotal" type="Double">
      <column name="subtotal" not-null="false" sql-type="double" />
    </property>
    <property name="isv" type="Double">
      <column name="isv" not-null="false" sql-type="double" />
    </property>
    <property name="descuento" type="Double">
      <column name="descuento" not-null="false" sql-type="double" />
    </property>
    <property name="total" type="Double">
      <column name="total" not-null="false" sql-type="double" />
    </property>
    <property name="id_cliente" type="Int32">
      <column name="id_cliente" not-null="true" precision="11" scale="0" sql-type="int" />
    </property>
    <many-to-one name="clientes" class="clientes">
      <column name="id_cliente" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
    <set name="detallle_facturas" inverse="true" generic="true">
      <key>
        <column name="num_factura" />
      </key>
      <one-to-many class="detallle_facturas" />
    </set>
  </class>
</hibernate-mapping>
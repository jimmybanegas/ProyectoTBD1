<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="account" table="account" schema="farmacia">
    <id name="Id" type="Int64">
      <column name="Id" not-null="true" precision="20" scale="0" sql-type="bigint" />
      <generator class="identity" />
    </id>
    <property name="FirstName" type="String">
      <column name="FirstName" not-null="false" length="255" sql-type="varchar" />
    </property>
    <property name="LastName" type="String">
      <column name="LastName" not-null="false" length="255" sql-type="varchar" />
    </property>
    <property name="Email" type="String">
      <column name="Email" not-null="false" length="255" sql-type="varchar" />
    </property>
    <property name="Password" type="String">
      <column name="`Password`" not-null="false" length="255" sql-type="varchar" />
    </property>
    <property name="EncryptKey" type="BinaryBlob">
      <column name="EncryptKey" not-null="false" sql-type="longblob" />
    </property>
    <property name="EncryptIV" type="BinaryBlob">
      <column name="EncryptIV" not-null="false" sql-type="longblob" />
    </property>
    <property name="IsArchived" type="Boolean">
      <column name="IsArchived" not-null="false" precision="1" scale="0" sql-type="tinyint" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <set name="account_has_grupos_roles" inverse="true" generic="true">
      <key>
        <column name="Id" />
      </key>
      <one-to-many class="account_has_grupos_roles" />
    </set>
    <set name="sessions" inverse="true" generic="true">
      <key>
        <column name="User_id" />
      </key>
      <one-to-many class="sessions" />
    </set>
  </class>
</hibernate-mapping>
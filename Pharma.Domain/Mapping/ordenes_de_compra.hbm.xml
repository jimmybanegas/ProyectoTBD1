<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ordenes_de_compra" table="ordenes_de_compra" schema="farmacia">
    <id name="id_ordencompra" type="Int32">
      <column name="id_ordencompra" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="fecha" type="DateTime">
      <column name="fecha" not-null="false" sql-type="date" />
    </property>
    <property name="total" type="Double">
      <column name="total" not-null="false" sql-type="double" />
    </property>
    <property name="id_proveedor" type="Int32">
      <column name="id_proveedor" not-null="true" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="ejecutada" type="Boolean">
      <column name="ejecutada" not-null="false" precision="1" scale="0" sql-type="tinyint" />
    </property>
    <property name="cancelada" type="Boolean">
      <column name="cancelada" not-null="false" precision="1" scale="0" sql-type="tinyint" />
    </property>
    <property name="activa" type="Boolean">
      <column name="activa" not-null="false" precision="1" scale="0" sql-type="tinyint" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <many-to-one name="proveedores" class="proveedores">
      <column name="id_proveedor" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
    <set name="detalle_ocs" inverse="true" generic="true">
      <key>
        <column name="id_ordencompra" />
      </key>
      <one-to-many class="detalle_oc" />
    </set>
  </class>
</hibernate-mapping>
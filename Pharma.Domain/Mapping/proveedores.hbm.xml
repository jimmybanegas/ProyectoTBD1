<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="proveedores" table="proveedores" schema="farmacia">
    <id name="id_proveedor" type="Int32">
      <column name="id_proveedor" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="direccion" type="String">
      <column name="direccion" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="telefono" type="String">
      <column name="telefono" not-null="false" length="15" sql-type="varchar" />
    </property>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="contacto" type="String">
      <column name="contacto" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <set name="compras" inverse="true" generic="true">
      <key>
        <column name="id_proveedor" />
      </key>
      <one-to-many class="compras" />
    </set>
    <set name="costo_prod_proveedors" inverse="true" generic="true">
      <key>
        <column name="id_proveedor" />
      </key>
      <one-to-many class="costo_prod_proveedor" />
    </set>
    <set name="cotizaciones" inverse="true" generic="true">
      <key>
        <column name="id_proveedor" />
      </key>
      <one-to-many class="cotizaciones" />
    </set>
    <set name="ordenes_de_compras" inverse="true" generic="true">
      <key>
        <column name="id_proveedor" />
      </key>
      <one-to-many class="ordenes_de_compra" />
    </set>
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="compras" table="compras" schema="farmacia">
    <id name="id_compra" type="Int32">
      <column name="id_compra" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="fecha" type="DateTime">
      <column name="fecha" not-null="false" sql-type="date" />
    </property>
    <property name="total" type="Double">
      <column name="total" not-null="false" sql-type="double" />
    </property>
    <property name="id_proveedor" type="Int32">
      <column name="id_proveedor" not-null="true" precision="11" scale="0" sql-type="int" />
    </property>
    <many-to-one name="proveedores" class="proveedores">
      <column name="id_proveedor" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
    <set name="detalle_compras" inverse="true" generic="true">
      <key>
        <column name="id_compra" />
      </key>
      <one-to-many class="detalle_compras" />
    </set>
  </class>
</hibernate-mapping>
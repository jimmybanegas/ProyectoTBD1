<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="usuarios" table="usuarios" schema="farmacia">
    <id name="usuario" type="String">
      <column name="usuario" not-null="true" length="45" sql-type="varchar" />
      <generator class="assigned" />
    </id>
    <property name="correo" type="String">
      <column name="correo" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="clave" type="Byte">
      <column name="clave" not-null="false" length="1" sql-type="binary" />
    </property>
    <property name="key" type="Byte">
      <column name="`key`" not-null="false" length="1" sql-type="binary" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <set name="usuarios_has_grupos_roles" inverse="true" generic="true">
      <key>
        <column name="usuario" />
      </key>
      <one-to-many class="usuarios_has_grupos_roles" />
    </set>
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Pharma.Domain" namespace="Pharma.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="opciones_modulos" table="opciones_modulos" schema="farmacia">
    <id name="id_opcion" type="Int32">
      <column name="id_opcion" not-null="true" precision="11" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="nombre" type="String">
      <column name="nombre" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="id_submodulo" type="Int32">
      <column name="id_submodulo" not-null="true" precision="11" scale="0" sql-type="int" />
    </property>
    <property name="fecha_crea" type="DateTime">
      <column name="fecha_crea" not-null="false" sql-type="datetime" />
    </property>
    <property name="fecha_actu" type="DateTime">
      <column name="fecha_actu" not-null="false" sql-type="datetime" />
    </property>
    <property name="usuario_crea" type="String">
      <column name="usuario_crea" not-null="false" length="45" sql-type="varchar" />
    </property>
    <property name="usuario_actu" type="String">
      <column name="usuario_actu" not-null="false" length="45" sql-type="varchar" />
    </property>
    <many-to-one name="submodulos" class="submodulos">
      <column name="id_submodulo" not-null="true" precision="11" scale="0" sql-type="int" />
    </many-to-one>
  </class>
</hibernate-mapping>